Img className='card-image' src={product.images[0].image1} />\r\n\r\n          <Card.Body>\r\n            <Card.Title as='p' className='name-label' style={{ background: 'black' , fontSize:'15px' }}>\r\n              <strong >{product.name}</strong>\r\n            </Card.Title>\r\n\r\n            <Card.Text as='h3' style={{ color: 'green',fontSize :'15px' }}>Rs {product.Cost.price}</Card.Text>\r\n          </Card.Body>\r\n        </Card>\r\n      </Link>\r\n    </>\r\n  )\r\n}\r\n\r\nexport default Product\r\n","import React from 'react'\r\nimport { Pagination } from 'react-bootstrap'\r\nimport { LinkContainer } from 'react-router-bootstrap'\r\nconst Paginate = ({ pages, page, isAdmin = false, keyword = ' ' }) => {\r\n  return (\r\n    pages > 1 && (\r\n      <Pagination className='paginate'>\r\n        {[...Array(pages).keys()].map((x) => (\r\n          <LinkContainer\r\n            key={x + 1}\r\n            to={\r\n              !isAdmin\r\n                ? keyword\r\n                  ? `/search/${keyword}/page/${x + 1}`\r\n                  : `/page/${x + 1}`\r\n                : `/admin/productlist/${x + 1}`\r\n            }\r\n          >\r\n            <Pagination.Item active={x + 1 === page}>{x + 1}</Pagination.Item>\r\n          </LinkContainer>\r\n        ))}\r\n      </Pagination>\r\n    )\r\n  )\r\n}\r\n\r\nexport default Paginate\r\n","import { Helmet } from 'react-helmet'\r\nimport React from 'react'\r\n\r\nconst Meta = ({ title, description, keywords }) => {\r\n  return (\r\n    <Helmet>\r\n      <title>{title}</title>\r\n      <meta name='description' content={description} />\r\n      <meta name='keyword' content={keywords} />\r\n    </Helmet>\r\n  )\r\n}\r\nMeta.defaultProps = {\r\n  title: 'Sell & Buy Books ',\r\n  description:\r\n    'Buy the second hand Books at cheap price ',\r\n  keyword: 'notes, Buy Used Books',\r\n}\r\nexport default Meta\r\n","import {\r\n  PRODUCT_LIST_REQUEST,\r\n  PRODUCT_LIST_SUCCESS,\r\n  PRODUCT_LIST_FAIL,\r\n  PRODUCT